<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Demo2Serverlet</servlet-name>
  	<display-name>Demo2Serverlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlets.Demo2Serverlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DetailServlet</servlet-name>
  	<display-name>DetailServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlets.DetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Demo2Serverlet</servlet-name>
  	<url-pattern>/Demo2Serverlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DetailServlet</servlet-name>
  	<url-pattern>/DetailServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  		<welcome-file>index5.jsp</welcome-file>
  </welcome-file-list>
</web-app>
