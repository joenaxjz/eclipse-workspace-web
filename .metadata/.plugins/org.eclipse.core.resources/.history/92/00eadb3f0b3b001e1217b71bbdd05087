			<th>Photo</th>
			<th>Category ID</th>
			<th>Category Name</th>



		</tr>
		<tr>
			<td><%=id%></td>
			<td><%=username%></td>
			<td><%=price%></td>
			<td><%=quantity%></td>
			<td><%=status%></td>
			<td>${quantity * price}</td>
			<td><img src="assets/images/${product.photo}" width="70px"
				height="100px"></td>
			<td>${product.category.id}</td>
			<td>${product.category.name}</td>
		</tr>
	</table>

